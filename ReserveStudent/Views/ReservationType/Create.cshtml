@model ReserveStudent.Models.ReservationType

@{
    ViewData["Title"] = "Create";
}

<h1></h1>

<h4> Create new Reservation Type</h4>
<hr />
<div class="row shadow-lg p-3 mb-5 bg-white rounded">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NameType" class="control-label"></label>
                <input asp-for="NameType" class="form-control" />
                <span asp-validation-for="NameType" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AccessNumber" class="control-label"></label>
                <input asp-for="AccessNumber" class="form-control" />
                <span asp-validation-for="AccessNumber  " class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
                <a asp-action="Index" class=" btn btn-outline-info">Back to List</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
